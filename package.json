{
  "name": "codescrolls",
  "version": "1.0.0",
  "private": true,
  "description": "A developer-focused documentation site built using Docusaurus.",
  "keywords": [
    "docusaurus",
    "documentation",
    "codescrolls",
    "developer-docs",
    "mdx",
    "typescript"
  ],
  "author": "Praveen Singh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/praveenskg/codescrolls.git"
  },
  "bugs": {
    "url": "https://github.com/praveenskg/codescrolls/issues"
  },
  "homepage": "https://codescrolls.site",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clean": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.1",
    "@docusaurus/faster": "^3.9.1",
    "@docusaurus/plugin-pwa": "^3.9.1",
    "@docusaurus/plugin-sitemap": "^3.9.1",
    "@docusaurus/plugin-vercel-analytics": "^3.9.1",
    "@docusaurus/preset-classic": "3.9.1",
    "@docusaurus/theme-live-codeblock": "^3.9.1",
    "@easyops-cn/docusaurus-search-local": "^0.52.1",
    "@mdx-js/react": "^3.1.1",
    "clsx": "^2.1.1",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-theme-openapi-docs": "^4.5.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.1",
    "@docusaurus/tsconfig": "3.9.1",
    "@docusaurus/types": "3.9.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,yml,yaml}": [
      "prettier --write"
    ]
  }
}
